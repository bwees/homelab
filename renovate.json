{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  "baseBranches": ["main"],
  "docker-compose": {
    "fileMatch": [
      "^compose/.*\\.ya?ml$"
    ]
  },
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["ghcr.io/lscr.io/linuxserver/qbittorrent"],
      "allowedVersions": "<20.04.1 || >20.04.1"
    }
  ]
}
